{
  "extends": "@nn-stack/config/tsconfig.base.json",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "jsx": "preserve",
    "incremental": true,
    "noEmit": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@nn-stack/ui/*": ["./src/*"]
    },
    "types": ["@cloudflare/workers-types", "./env.d.ts"]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "alchemy.run.ts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
